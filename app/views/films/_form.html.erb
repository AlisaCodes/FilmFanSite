<%= render "layouts/errors", :object => @film %>

<%= form_for @film do |f| %>
  <%= f.label :title %>
  <%= f.text_field :title%>
  <%= f.label :director %>
  <%= f.text_field :director%>
  <%= f.label :url %>
  <%= f.text_field :url%>
  <%= f.label :description %>
  <%= f.text_field :description%>
  <%= f.submit %>
<% end %>
